%{
	#include<stdio.h>
%}
%token INTEGER
%%
p:p e '\n'      {printf("%d \n",$2);}
    |
    ;
e : e '*' f     {$$ = $1 * $3;}
  | f           {$$ = $1;}
  ;
f : g '+' f     {$$ = $1 + $3;}
  | g           {$$ = $1;}
  ;
g : g '-' h     {$$ = $1 - $3;}
  | h           {$$ = $1;}
  ;
h : INTEGER      {$$ = $1;}
  ;
%%
int main()
{
	yyparse();
    return 0;
}
void yyerror(char *s)
{
	fprintf(stderr,"%s\n",s);
}
